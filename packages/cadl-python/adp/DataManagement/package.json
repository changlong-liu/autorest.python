{
  "name": "@cadl-api-spec/adp-data-management",
  "version": "1.0",
  "description": "ADP Data Management API",
  "private": true,
  "main": "index.js",
  "cadlMain": "client.cadl",
  "config": {
    "verbose": " -Verbose",
    "DataManagementOutputDirectory": "../data-plane/Adp.DataManagement",
    "DataManagementPublicOutputFile": "preview/2022-11-30-preview/adp-data-management-api.json",
    "PublicClientPath": "../generated/clients/DataManagement/Clients.DataManagement.vDraftCompliant",
    "PublicClientNamespace": "Microsoft.Azure.Adp.DataManagement.Clients.vDraftCompliant",
    "PublishDataManagementPublicOutputFileName": "../../../../draftcompliant/apispec.core.datamanagement.2022-11-30-draftcompliant.json",
    "PathToAzureApiReviewRepository": "C:/Users/igmakhli/source/repos/imakhlin/azure-rest-api-specs-pr/specification/adp/data-plane/Adp.DataManagement/preview",
    "AutorestManifestFile": "../src/scripts/autorest.md"
  },
  "scripts": {
    "lint": "cadl compile . --warn-as-error --import @cadl-lang/library-linter --no-emit",
    "format": "npm run _formatCadl && npm run _formatJs",
    "validate": "npm run _validateOpenApiSpec && npm run _validateExamples",
    "build": "npm run format && npx cadl compile . && npm run validate",
    "publish": "pwsh ../src/scripts/publish.ps1 %npm_package_config_verbose% -SourceOpenApiSpecPath %npm_package_config_DataManagementOutputDirectory%/%npm_package_config_DataManagementPublicOutputFile% -OutputOpenApiSpecPath %npm_package_config_PublishDataManagementPublicOutputFileName% -PathToAzureApiReviewRepository %npm_package_config_PathToAzureApiReviewRepository%",
    "check-version-mismatch": "syncpack list-mismatches",
    "fix-version-mismatch": "syncpack fix-mismatches",
    "_refreshVSToken": "vsts-npm-auth -config .npmrc",
    "_validateOpenApiSpec": "pwsh ../src/scripts/validate-spec.ps1 -OpenApiSpecFile %npm_package_config_DataManagementOutputDirectory%/%npm_package_config_DataManagementPublicOutputFile%",
    "_validateExamples": "pwsh ../src/scripts/validate-examples.ps1 -OpenApiSpecFile %npm_package_config_DataManagementOutputDirectory%/%npm_package_config_DataManagementPublicOutputFile%",
    "_compile": "pwsh ../src/scripts/compile.ps1 -IsPublicApi -InputCadl . -OutputPath %npm_package_config_DataManagementOutputDirectory% -OutputFile %npm_package_config_DataManagementPublicOutputFile%",
    "_generateClientWithAutoRest": "pwsh ../src/scripts/generate-autorest-client.ps1 -UseLatestAutoRestVersion -InputOpenApiSpecFile %npm_package_config_DataManagementOutputDirectory%/%npm_package_config_DataManagementPublicOutputFile% -OutputPath %npm_package_config_PublicClientPath%/generated -Namespace %npm_package_config_PublicClientNamespace% -AutorestManifestFile %npm_package_config_AutorestManifestFile%",
    "_generateExamples": "pwsh ../src/scripts/generate-examples.ps1 -OpenApiSpecFile %npm_package_config_DataManagementOutputDirectory%/%npm_package_config_DataManagementPublicOutputFile%",
    "_formatCadl": "npx cadl format --pretty ./**/*.cadl",
    "_formatJs": "prettier --write \"**/*.+(js|json)\""
  },
  "keywords": [],
  "author": "igmakhli@microsoft.com",
  "license": "MIT",
